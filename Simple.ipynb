    "#0と19の最大公約数は19。0は何をかけてもゼロだからすべての数字の約数といえる。\n",
    "def Euclid(x1,x2):\n",
    "    flag=True\n",
    "    while flag==True:\n",
    "        if x1==0 or x2 ==0:\n",
    "            x1,x2=0,0\n",
    "            flag=False\n",
    "        elif x1<x2:\n",
    "            x2=x2-x1\n",
    "            print(\"x1は{}でx2は{}\".format(x1,x2))\n",
    "        elif x1>x2:\n",
    "            x1=x1-x2\n",
    "            print(\"x1は{}でx2は{}\".format(x1,x2)) \n",
    "        \n",
    "        else:\n",
    "            flag=False\n",
    "    return x1\n",
    "\n",
    "print(Euclid(0,44))"
